<dt> <%= t(".label", type: type.humanize) %> </dt>
<dd>
    <% if items.blank? %>
      <p><%= t('.empty', type: type.humanize) %></p>
    <% else %>
      <ul class="tabular">
        <% items.each do |item| %>
          <li class='attribute attribute-title'>
            <%= link_to item.title.first, search_catalog_path("f[admin_set_tesim][]": item.title.first) %>
          </li>
        <% end %>
      </ul>
    <% end %>
  </dd>